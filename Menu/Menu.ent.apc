PROC ME_ED
onfocus
	//displays saved ED  number

	$ = tonumber(loadsetting("ED")); 
	
postproc
	rnd_ed = maketext("%v%v",ME_ROUND,ME_ED);
	//saves the ED number as a setting
	savesetting("ED",maketext("%d",$));
