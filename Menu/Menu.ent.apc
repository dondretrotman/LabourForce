PROC UNCHANGED_HOUSEHOLDS
onfocus
	numeric nextEntry = 1;
	numeric headindex;
	forcase LABOURFORCE_DICT where LSTED = ME_ED and LSTROUND = ME_ROUND do
		loadcase(LABOURFORCE_DICT,RNDNO,EDNO,PARNO,STRATUM,HHNO);
		if NO_CHANGE = 1 then		//if a no change, include name
			headindex = seek(LRTH = 0);
			vscode(nextEntry) = maketext("%V%V%V%V%V",RNDNO,EDNO,PARNO,STRATUM,HHNO);
			vslabel(nextEntry) = maketext("%S-%02d-%03d-%03d-%d-%03d",strip(LLAST_NAME(headindex)),RNDNO,EDNO,PARNO,STRATUM,HHNO );
			trace("nextEntry = %d",nextEntry);
			trace("%S-%02d-%03d-%03d-%d-%03d",strip(LLAST_NAME(headindex)),RNDNO,EDNO,PARNO,STRATUM,HHNO);
			inc(nextEntry);
		endif;
	endfor;
	
	vscode(nextEntry) = "";
	setvalueset($,vsCode,vsLabel);
	
	{if vscode(1) = "" then		//if nothing is in the valueset
		errmsg("There are no questionnaires available that fit this criteria.");
		move to SUPERVISOR_MENU;
	endif;}

postproc
	move to SUPERVISOR_MENU;


