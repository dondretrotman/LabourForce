//downloads unchanged households and then displays them
function getunchanged()
	errflag = 0;
	i = 1;		//counot number to unchanged households
	j = 1;		//count number of downloaded unchnaged households
	while i <> 100 do		//reset array
		nocid(i) = "";
		inc(i);
	enddo;
	i = 1;
	//get unchanged household ids
	forcase LABOURFORCE_DICT where NO_CHANGE = 1 do
		nocid(i) = maketext("%V%V%V%V%V",RNDNO-1,EDNO,PARNO,STRATUM,HHNO);
		trace("i nocid(%d) = %s",i,nocid(i));
		inc(i);
	endfor;
	if nocid(1) = "" then
		break;
	endif;
	//download unchanged households
	if syncconnect(CSWeb,cswebip1,ftpun,ftppw) then		//connect to FTP when within the Government WAN
		while j < i do
			if not syncdata(GET,LABOURFORCE_DICT,maketext("%12s",nocid(j))) then		//download Questionnaire from previous round
				trace("j nocid(%d) = %s",j,nocid(j));
				inc(j);
				inc(errflag);
			endif;
		enddo;
		syncdisconnect();		//disconnect
	elseif syncconnect(CSWeb,cswebip2,ftpun,ftppw) then		//connect to FTP when within the Government WAN
		while j < i do
			if not syncdata(GET,LABOURFORCE_DICT,maketext("%12s",nocid(j))) then		//download Questionnaire from previous round
				trace("j nocid(%d) = %s",j,nocid(j));
				inc(j);
				inc(errflag);
			endif;
		enddo;
		syncdisconnect();		//disconnect
	else
		inc(errflag,2);
	endif;
	if errflag >= 1 then
		errmsg("Data download failed, please try again. %d errors were found",errflag);
	elseif errflag = 0 then
		errmsg("Data download was successful");
	endif;
end;

